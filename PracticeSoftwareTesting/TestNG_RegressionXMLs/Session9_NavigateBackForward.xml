<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="com.shubhamklogic.selenium.practice.browserOpen">
  
  <parameter name="browsers" value="chrome"/>
	  	<!-- 
	 		Desc: Parameter=browsers
	 		This parameter configures at which browser the test cases would be executed.
	 	-->
  
  <parameter name="browserDriverSetupMode" value="webDriverManager" />
	  	<!-- 
			Desc: Parameter=browserDriverSetupMode 
			This parameter tests the setup the browser driver via = webDriverManager / executableBrowserDrivers
	  	-->
  
  <test name="ShubhamKLogic.com : Navigation Test">
  		<groups>
  			<run>
  				<include name="current" />
  			</run>
  		</groups>
  		<classes>
  			<class name="com.shubhamklogic.practice.selenium.NavigationTest"/>
  		</classes>
  </test>
</suite>