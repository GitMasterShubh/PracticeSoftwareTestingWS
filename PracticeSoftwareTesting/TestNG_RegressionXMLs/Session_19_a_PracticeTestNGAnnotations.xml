<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="com.shubhamklogic.selenium.practice">
  
  <parameter name="browsers" value="chrome"/>
	  	<!-- 
	 		Desc: Parameter=browsers
	 		This parameter configures at which browser the test cases would be executed.
	 	-->
  
  <parameter name="browserDriverSetupMode" value="executableBrowserDrivers" />
	  	<!-- 
			Desc: Parameter=browserDriverSetupMode 
			This parameter tests the setup the browser driver via = webDriverManager / executableBrowserDrivers
	  	-->
  
  <test name="ShubhamKLogic.com : Understand All TestNG Annotations">
  		<!-- 
	  		To run any specific test case, 
	  		1. Uncomment below <groups> tag code
	  		2. Go to your below test class, and add word "current" in @test cases' attribute : groups, Eg: groups = {"current", "other-group-values"}
	  		
	  		^ To Be Done by you ^
	  	-->
  		<!-- <groups>
  			<run>
  				<include name="current" />
  			</run>
  		</groups> -->
  		<classes>
  			<class name="com.shubhamklogic.practice.testng.PracticeTestNGAnnotationsTest"/>
  		</classes>
  </test>
</suite>